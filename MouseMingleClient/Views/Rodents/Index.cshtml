<!-- all the rodents -->

@{
  ViewData["Title"] = "Everyone";
}

@using MouseMingleClient.Models;

@if (@Model.Count == 0)
{
  <p>No Rodents Yet</p>
}
  <ol>
    @foreach (Rodent rodent in Model)
    {
      <li><strong>@rodent.Name</strong> the @rodent.Species | <a href="/Rodents/Details/@(rodent.RodentId)">View Details</a>
      </li>
    }
  </ol>